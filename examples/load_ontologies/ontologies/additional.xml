<?xml version="1.0" encoding="utf-8" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

  <owl:Ontology rdf:about="http://www.bloodbee.space/ontology/stuff/1.1">
  </owl:Ontology>

  <owl:ObjectProperty rdf:about="http://www.bloodbee.space/ontology/stuff/1.1#color">
    <rdfs:domain rdf:resource="http://www.bloodbee.space/ontology/stuff/1.1#LivingThing"/>
    <rdfs:domain>
      <owl:Class rdf:about="http://www.bloodbee.space/ontology/stuff/1.1#Object">
      </owl:Class>
    </rdfs:domain>

    <rdfs:range>
      <owl:Class rdf:about="http://www.bloodbee.space/ontology/stuff/1.1#Color">
      </owl:Class>
    </rdfs:range>

    <rdfs:comment xml:lang="en">the color of the entity</rdfs:comment>
    <rdfs:label xml:lang="en">color</rdfs:label>
  </owl:ObjectProperty>

  <owl:DatatypeProperty rdf:about="http://www.bloodbee.space/ontology/stuff/1.1#isHeliophilous">
    <rdfs:domain rdf:resource="http://www.bloodbee.space/ontology/stuff/1.1#Plants"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:comment xml:lang="en">love the sun</rdfs:comment>
    <rdfs:label xml:lang="en">heliophile</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.bloodbee.space/ontology/stuff/1.1#isQueen">
    <rdfs:domain>
      <owl:Class rdf:about="http://www.bloodbee.space/ontology/stuff/1.1#Bee">
        <rdfs:subClassOf>
          <owl:Class rdf:about="http://www.bloodbee.space/ontology/stuff/1.1#Mosquito">
            <rdfs:subClassOf rdf:resource="http://www.bloodbee.space/ontology/stuff/1.1#LivingThing"/>
          </owl:Class>
        </rdfs:subClassOf>

      </owl:Class>
    </rdfs:domain>

    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:comment xml:lang="en">boolean to know if the bee is a queen</rdfs:comment>
    <rdfs:label xml:lang="en">if the entity is the queen</rdfs:label>
  </owl:DatatypeProperty>

  <owl:Class rdf:about="http://www.bloodbee.space/ontology/stuff/1.1#Cactus">
    <rdfs:subClassOf rdf:resource="http://www.bloodbee.space/ontology/stuff/1.1#Plants"/>
  </owl:Class>

  <owl:Class rdf:about="http://www.bloodbee.space/ontology/stuff/1.1#LivingThing">
  </owl:Class>

  <owl:Class rdf:about="http://www.bloodbee.space/ontology/stuff/1.1#Plants">
    <rdfs:subClassOf rdf:resource="http://www.bloodbee.space/ontology/stuff/1.1#LivingThing"/>
  </owl:Class>

  <owl:Class rdf:about="http://www.bloodbee.space/ontology/stuff/1.1#Rose">
    <rdfs:subClassOf rdf:resource="http://www.bloodbee.space/ontology/stuff/1.1#Plants"/>
  </owl:Class>

</rdf:RDF>